                        "value": "4h"
                    },
                    {
                        "selected": false,
                        "text": "5h",
                        "value": "5h"
                    },
                    {
                        "selected": false,
                        "text": "6h",
                        "value": "6h"
                    },
                    {
                        "selected": false,
                        "text": "12h",
                        "value": "12h"
                    },
                    {
                        "selected": false,
                        "text": "1d",
                        "value": "1d"
                    },
                    {
                        "selected": false,
                        "text": "2d",
                        "value": "2d"
                    },
                    {
                        "selected": false,
                        "text": "3d",
                        "value": "3d"
                    },
                    {
                        "selected": false,
                        "text": "4d",
                        "value": "4d"
                    },
                    {
                        "selected": false,
                        "text": "7d",
                        "value": "7d"
                    }
                ],
                "query": "1h : 1h,2h : 2h,3h : 3h,4h : 4h,5h : 5h,6h : 6h,12h : 12h,1d : 1d,2d : 2d,3d : 3d,4d : 4d,7d : 7d",
                "type": "custom"
            },
            {
                "datasource": {
                    "type": "prometheus",
                    "uid": "${source}"
                },
                "multi": false,
                "name": "instance",
                "query": "query_result(sort_desc(    sum by (instance) (increase(    youtrack_UserSessions_ActiveUsers{}[$__range:$__interval]    ))))",
                "refresh": 2,
                "regex": "/{instance=\"(.*)\"}.*/",
                "type": "query"
            },
            {
                "current": {
                    "selected": false,
                    "text": "‚úÖ Yes",
                    "value": "1"
                },
                "label": "Show app start",
                "multi": false,
                "name": "app_start",
                "options": [
                    {
                        "selected": true,
                        "text": "‚úÖ Yes",
                        "value": "1"
                    },
                    {
                        "selected": false,
                        "text": "üî≥ No",
                        "value": "0"
                    }
                ],
                "query": "‚úÖ Yes : 1,üî≥ No : 0",
                "type": "custom"
            },
            {
                "current": {
                    "selected": false,
                    "text": "Rule",
                    "value": "‚ùáÔ∏è Rule"
                },
                "label": "Workflow group",
                "multi": false,
                "name": "workflow_group",
                "options": [
                    {
                        "selected": true,
                        "text": "RuleGuard",
                        "value": "üõ° Rule Guard"
                    },
                    {
                        "selected": false,
