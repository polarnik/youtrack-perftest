                    },
                    {
                        "selected": false,
                        "text": "OnScheduleFull",
                        "value": "üóì On Schedule Full"
                    }
                ],
                "query": "RuleGuard : üõ° Rule Guard,Rule : ‚ùáÔ∏è Rule,OnScheduleFull : üóì On Schedule Full",
                "type": "custom"
            },
            {
                "datasource": {
                    "type": "prometheus",
                    "uid": "${source}"
                },
                "multi": false,
                "name": "script",
                "query": "query_result(sort_desc(sum_over_time((sum(increase(youtrack_Workflow_${workflow_group:text}_TotalDuration{instance=~\"$instance\"}[1m:])) by (script))[$__range:1m])))",
                "refresh": 2,
                "regex": "/{script=\"(.*)\"}.*/",
                "type": "query"
            }
        ]
    },
    "time": {
        "from": "now-6h",
        "to": "now"
    },
    "timezone": "utc",
    "title": "YouTrack Workflow Details",
    "uid": "yt_workflow_details"
}