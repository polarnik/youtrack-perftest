{
    "description": "Dashboard with details about some YouTrack Workflow\n",
    "graphTooltip": 1,
    "links": [
        {
            "asDropdown": true,
            "icon": "external link",
            "includeVars": true,
            "keepTime": true,
            "tags": [
                "YouTrack Server"
            ],
            "targetBlank": true,
            "title": "YouTrack Server",
            "tooltip": "",
            "type": "dashboards",
            "url": ""
        },
        {
            "asDropdown": true,
            "icon": "external link",
            "includeVars": true,
            "keepTime": true,
            "tags": [
                "YouTrack Workflows"
            ],
            "targetBlank": true,
            "title": "YouTrack Workflows",
            "tooltip": "",
            "type": "dashboards",
            "url": ""
        }
    ],
    "panels": [
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 0,
                "x": 0,
                "y": 0
            },
            "id": 1,
            "title": "Version",
            "type": "row"
        },
        {
            "datasource": {
                "type": "datasource",
                "uid": "-- Mixed --"
            },
            "gridPos": {
                "h": 7,
                "w": 4,
                "x": 0,
                "y": 1
            },
            "id": 2,
            "options": {
                "code": {
                    "language": "plaintext",
                    "showLineNumbers": false,
                    "showMiniMap": false
                },
                "content": "### Version of <a href=\"https://www.jetbrains.com/youtrack/\" target=\"_blank\" rel=\"author\" title=\"YouTrack. Project management for all your teams\"><img src=\"https://youtrack.jetbrains.com/youtrack-wide-dark.svg\" height=\"30\" /></a>\nDifferent colours üü©üü®üü¶ are different versions.\n\nMove a cursor üëÜto the head of the line to get version info.\n",
                "mode": "markdown"
            },
            "pluginVersion": "v10.4.0",
            "title": "",
            "type": "text"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "${source}"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisWidth": 65,
                        "drawStyle": "line",
                        "fillOpacity": 100,
                        "gradientMode": "opacity",
                        "lineWidth": 10,
                        "showPoints": "never"
                    },
                    "decimals": 0,
                    "max": 1,
                    "min": 0,
                    "unit": "none"
                }
            },
            "gridPos": {
                "h": 7,
                "w": 20,
                "x": 4,
                "y": 1
            },
            "id": 3,
            "interval": "3m",
            "maxDataPoints": 300,
            "options": {
                "legend": {
                    "isVisible": false,
                    "showLegend": false
                },
                "tooltip": {
                    "hoverProximity": 30,
                    "mode": "single",
                    "sort": "desc"
                }
            },
            "pluginVersion": "v10.4.0",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "${source}"
                    },
                    "expr": "avg by (Version) ({__name__=~\"youtrack_version_info|youtrack_version\",  instance=~\"$instance\" })",
                    "format": "table",
                    "instant": false,
                    "range": true,
                    "refId": "Version"
                },
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "${source}"
                    },
                    "expr": "avg by (instance, Build) ({__name__=~\"youtrack_version_info|youtrack_version\",  instance=~\"$instance\" })",
                    "format": "table",
                    "instant": false,
                    "range": true,
                    "refId": "Build"
                }
            ],
            "title": "Version",
            "transformations": [
                {
                    "id": "merge",
                    "options": { }
                },
                {
                    "id": "filterByValue",
                    "options": {
                        "filters": [
                            {
                                "config": {
                                    "id": "regex",
                                    "options": {
                                        "value": ".+"
                                    }
                                },
                                "fieldName": "Version"
                            },
                            {
                                "config": {
                                    "id": "regex",
                                    "options": {
                                        "value": ".+"
                                    }
                                },
                                "fieldName": "Build"
                            }
                        ],
                        "match": "all",
                        "type": "include"
                    }
                },
                {
                    "id": "organize",
                    "options": {
                        "excludeByName": {
                            "Value #Build": true,
                            "instance": true
                        },
                        "includeByName": { },
                        "indexByName": {
                            "Build": 3,
                            "Time": 0,
                            "Value #Build": 5,
                            "Value #Version": 4,
                            "Version": 2,
                            "instance": 1
                        },
                        "renameByName": {
                            "Value #Version": "Version info"
                        }
                    }
                },
                {
                    "id": "prepareTimeSeries",
                    "options": {
                        "format": "multi"
                    }
                }
            ],
            "type": "timeseries"
        }
    ],
    "schemaVersion": 36,
    "tags": [
        "YouTrack Server",
        "YouTrack Workflows"
    ],
    "templating": {
        "list": [
            {
                "name": "source",
                "query": "prometheus",
                "type": "datasource"
            },
            {
                "current": {
                    "selected": false,
                    "text": "7d",
